    "directory": "packages/erc8004-sdk"
  },
  "dependencies": {
    "viem": "^2.0.0",
    "ethers": "^6.0.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0"
  },
  "peerDependencies": {
    "viem": ">=2.0.0",
    "ethers": ">=6.0.0"
  },
  "engines": {
