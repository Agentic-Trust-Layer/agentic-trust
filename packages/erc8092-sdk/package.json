{
  "name": "@agentic-trust/8092-sdk",
  "version": "1.0.42",
  "private": true,
  "description": "ERC-8092 Associated Accounts SDK - A TypeScript SDK for interacting with ERC-8092 compliant implementations",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist tsconfig.tsbuildinfo"
  },
  "peerDependencies": {
    "ethers": "^6.0.0"
  },
  "devDependencies": {
    "ethers": "^6.13.4",
    "tsup": "^8.5.0",
    "typescript": "^5.7.2"
  }
}
