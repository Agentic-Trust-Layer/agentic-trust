{
  "name": "@agentic-trust/admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p ${PORT:-3002}",
    "build": "next build",
    "start": "next start -p ${PORT:-3002}",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:integration": "INTEGRATION_TESTS=true vitest run --config vitest.integration.config.ts",
    "test:integration:watch": "INTEGRATION_TESTS=true vitest --config vitest.integration.config.ts",
    "test:all": "pnpm test && pnpm test:integration"
  },
  "dependencies": {
    "@agentic-trust/core": "workspace:*",
    "@agentic-trust/veramo-agent-extension": "workspace:*",
    "@emotion/cache": "^11.11.3",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.3",
    "@metamask/smart-accounts-kit": "^0.1.0",
    "@mui/icons-material": "^5.15.17",
    "@mui/material": "^5.15.17",
    "@veramo/core": "^6.0.0",
    "@veramo/did-manager": "^6.0.0",
    "@veramo/did-resolver": "^6.0.0",
    "@veramo/key-manager": "^6.0.0",
    "@web3auth/base": "^8.12.4",
    "@web3auth/ethereum-provider": "^8.12.4",
    "@web3auth/metamask-adapter": "^8.12.4",
    "@web3auth/modal": "^8.12.4",
    "@web3auth/openlogin-adapter": "^8.12.4",
    "@xyflow/react": "^12.10.0",
    "did-resolver": "^4.1.0",
    "ethr-did-resolver": "^9.0.0",
    "next": "^14.2.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "viem": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitest/coverage-v8": "^2.1.9",
    "@vitest/ui": "^2.0.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "typescript": "^5.3.0",
    "vitest": "^2.0.0"
  }
}
