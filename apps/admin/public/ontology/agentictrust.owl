@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix p-plan: <http://purl.org/net/p-plan#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix agentictrust: <https://www.agentictrust.io/ontology/agentictrust#> .

agentictrust: a owl:Ontology ;
  dcterms:title "AgenticTrustOntologyAgentCardsIntentMessaging" ;
  dcterms:description "Ontology for Agentic Trust concepts: ERC-8004 agents, A2A agent cards, skills, intent-driven messaging, tasks/workflows, and provenance. Grounded in PROV-O and P-PLAN." ;
  owl:imports <http://www.w3.org/ns/prov-o#> ;
  owl:imports <http://purl.org/net/p-plan#> ;
  owl:versionInfo "0.2.0" ;
  dcterms:license <https://opensource.org/license/mit/> .

########
# Core Agent / Agent Card concepts
########

agentictrust:ERC8004Agent a owl:Class ;
  rdfs:label "ERC8004Agent" ;
  rdfs:subClassOf prov:SoftwareAgent .

agentictrust:AgentRegistration a owl:Class ;
  rdfs:label "ERC8004RegistrationJSON" ;
  rdfs:comment "Registration metadata (typically from tokenUri) that includes endpoints (e.g., A2A registration endpoint)." ;
  rdfs:subClassOf prov:Entity .

agentictrust:AgentCard a owl:Class ;
  rdfs:label "A2AAgentCard" ;
  rdfs:comment "A2A protocol agent card (agent.json / agent-card.json) describing skills, transports, and capabilities." ;
  rdfs:subClassOf prov:Entity .

agentictrust:Endpoint a owl:Class ;
  rdfs:label "Endpoint" ;
  rdfs:comment "Network endpoint used by A2A/MCP." ;
  rdfs:subClassOf prov:Entity .

agentictrust:TransportBinding a owl:Class ;
  rdfs:label "TransportBinding" ;
  rdfs:comment "Transport/protocol binding (e.g., HTTP+JSON, JSON-RPC)." ;
  rdfs:subClassOf prov:Entity .

agentictrust:Skill a owl:Class ;
  rdfs:label "A2ASkill" ;
  rdfs:comment "A callable capability described in an A2A agent card." ;
  rdfs:subClassOf prov:Entity .

agentictrust:SkillExample a owl:Class ;
  rdfs:label "SkillExample" ;
  rdfs:comment "Example attached to a skill; may contain intent-driven requests." ;
  rdfs:subClassOf prov:Entity .

agentictrust:Tag a owl:Class ;
  rdfs:label "Tag" ;
  rdfs:comment "A tag value used for discovery and matching (e.g., trust.validation, ens.name.valid)." ;
  rdfs:subClassOf prov:Entity .

########
# Intent-driven message model
########

agentictrust:Message a owl:Class ;
  rdfs:label "A2AMessage" ;
  rdfs:comment "A message exchanged between agents; carries content parts." ;
  rdfs:subClassOf prov:Entity .

agentictrust:MessagePart a owl:Class ;
  rdfs:label "MessagePart" ;
  rdfs:comment "Part of a message (text or structured data)." ;
  rdfs:subClassOf prov:Entity .

agentictrust:TextPart a owl:Class ;
  rdfs:label "TextPart" ;
  rdfs:subClassOf agentictrust:MessagePart .

agentictrust:DataPart a owl:Class ;
  rdfs:label "DataPart" ;
  rdfs:subClassOf agentictrust:MessagePart .

agentictrust:Intent a owl:Class ;
  rdfs:label "Intent" ;
  rdfs:comment "Structured intent payload carried in a message data-part. Also treated as a plan (P-PLAN) to support workflow reasoning." ;
  rdfs:subClassOf prov:Entity ;
  rdfs:subClassOf p-plan:Plan .

agentictrust:IntentType a owl:Class ;
  rdfs:label "IntentType" ;
  rdfs:comment "Taxonomy value used to scope discovery and select compatible skills." ;
  rdfs:subClassOf prov:Entity .

agentictrust:Action a owl:Class ;
  rdfs:label "IntentAction" ;
  rdfs:comment "Action/verb used within an intent." ;
  rdfs:subClassOf prov:Entity .

agentictrust:Subject a owl:Class ;
  rdfs:label "IntentSubject" ;
  rdfs:comment "The subject of an intent (e.g., an agent ENS name)." ;
  rdfs:subClassOf prov:Entity .

agentictrust:Check a owl:Class ;
  rdfs:label "IntentCheck" ;
  rdfs:comment "A requested check within an intent." ;
  rdfs:subClassOf prov:Entity .

agentictrust:ExpectedResult a owl:Class ;
  rdfs:label "ExpectedResult" ;
  rdfs:subClassOf prov:Entity .

agentictrust:Evidence a owl:Class ;
  rdfs:label "Evidence" ;
  rdfs:subClassOf prov:Entity .

agentictrust:Artifact a owl:Class ;
  rdfs:label "ResultArtifact" ;
  rdfs:comment "Artifact produced as a result of executing an intent (e.g., attestation, validation-result)." ;
  rdfs:subClassOf prov:Entity .

agentictrust:Attestation a owl:Class ;
  rdfs:label "Attestation" ;
  rdfs:subClassOf agentictrust:Artifact .

########
# Task / workflow model (P-PLAN + PROV)
########

agentictrust:TaskType a owl:Class ;
  rdfs:label "TaskType" ;
  rdfs:comment "Workflow/task type derived from skills/intent taxonomy." ;
  rdfs:subClassOf prov:Entity .

agentictrust:Task a owl:Class ;
  rdfs:label "Task" ;
  rdfs:comment "A task thread or work item created by intent messaging." ;
  rdfs:subClassOf prov:Entity .

agentictrust:TaskExecution a owl:Class ;
  rdfs:label "TaskExecution" ;
  rdfs:comment "Execution activity that realizes a task plan." ;
  rdfs:subClassOf prov:Activity .

agentictrust:MessageSend a owl:Class ;
  rdfs:label "MessageSend" ;
  rdfs:subClassOf prov:Activity .

agentictrust:MessageReceive a owl:Class ;
  rdfs:label "MessageReceive" ;
  rdfs:subClassOf prov:Activity .

agentictrust:SkillInvocation a owl:Class ;
  rdfs:label "SkillInvocation" ;
  rdfs:comment "JSON-RPC based invocation of a skill at an A2A service endpoint." ;
  rdfs:subClassOf prov:Activity .

agentictrust:AgentCardFetch a owl:Class ;
  rdfs:label "AgentCardFetch" ;
  rdfs:comment "Indexing activity that retrieves an agent card from a registration endpoint." ;
  rdfs:subClassOf prov:Activity .

########
# Object properties
########

agentictrust:hasAgentRegistration a owl:ObjectProperty ;
  rdfs:label "hasAgentRegistration" ;
  rdfs:domain agentictrust:ERC8004Agent ;
  rdfs:range agentictrust:AgentRegistration .

agentictrust:hasAgentCard a owl:ObjectProperty ;
  rdfs:label "hasAgentCard" ;
  rdfs:domain agentictrust:ERC8004Agent ;
  rdfs:range agentictrust:AgentCard .

agentictrust:hasEndpoint a owl:ObjectProperty ;
  rdfs:label "hasEndpoint" ;
  rdfs:domain agentictrust:AgentCard ;
  rdfs:range agentictrust:Endpoint .

agentictrust:hasSkill a owl:ObjectProperty ;
  rdfs:label "hasSkill" ;
  rdfs:domain agentictrust:AgentCard ;
  rdfs:range agentictrust:Skill .

agentictrust:hasExample a owl:ObjectProperty ;
  rdfs:label "hasExample" ;
  rdfs:domain agentictrust:Skill ;
  rdfs:range agentictrust:SkillExample .

agentictrust:hasTag a owl:ObjectProperty ;
  rdfs:label "hasTag" ;
  rdfs:domain agentictrust:Skill ;
  rdfs:range agentictrust:Tag .

agentictrust:supportsIntentType a owl:ObjectProperty ;
  rdfs:label "supportsIntentType" ;
  rdfs:domain agentictrust:Skill ;
  rdfs:range agentictrust:IntentType .

agentictrust:hasIntent a owl:ObjectProperty ;
  rdfs:label "hasIntent" ;
  rdfs:domain agentictrust:Message ;
  rdfs:range agentictrust:Intent .

agentictrust:hasPart a owl:ObjectProperty ;
  rdfs:label "hasPart" ;
  rdfs:domain agentictrust:Message ;
  rdfs:range agentictrust:MessagePart .

agentictrust:hasSubject a owl:ObjectProperty ;
  rdfs:label "hasSubject" ;
  rdfs:domain agentictrust:Intent ;
  rdfs:range agentictrust:Subject .

agentictrust:hasCheck a owl:ObjectProperty ;
  rdfs:label "hasCheck" ;
  rdfs:domain agentictrust:Intent ;
  rdfs:range agentictrust:Check .

agentictrust:hasEvidence a owl:ObjectProperty ;
  rdfs:label "hasEvidence" ;
  rdfs:domain agentictrust:Intent ;
  rdfs:range agentictrust:Evidence .

agentictrust:hasExpectedResult a owl:ObjectProperty ;
  rdfs:label "hasExpectedResult" ;
  rdfs:domain agentictrust:Intent ;
  rdfs:range agentictrust:ExpectedResult .

agentictrust:producedArtifact a owl:ObjectProperty ;
  rdfs:label "producedArtifact" ;
  rdfs:domain agentictrust:TaskExecution ;
  rdfs:range agentictrust:Artifact .

agentictrust:taskHasType a owl:ObjectProperty ;
  rdfs:label "taskHasType" ;
  rdfs:domain agentictrust:Task ;
  rdfs:range agentictrust:TaskType .

agentictrust:taskRealizedBy a owl:ObjectProperty ;
  rdfs:label "taskRealizedBy" ;
  rdfs:domain agentictrust:Task ;
  rdfs:range agentictrust:TaskExecution .

agentictrust:invokesSkill a owl:ObjectProperty ;
  rdfs:label "invokesSkill" ;
  rdfs:domain agentictrust:SkillInvocation ;
  rdfs:range agentictrust:Skill .

agentictrust:invocationUsedMessage a owl:ObjectProperty ;
  rdfs:label "invocationUsedMessage" ;
  rdfs:domain agentictrust:SkillInvocation ;
  rdfs:range agentictrust:Message .

agentictrust:hadPlan a owl:ObjectProperty ;
  rdfs:label "hadPlan" ;
  rdfs:comment "Alias to prov:hadPlan to connect executions to intent/task plans." ;
  rdfs:subPropertyOf prov:hadPlan ;
  rdfs:domain prov:Activity ;
  rdfs:range prov:Plan .

