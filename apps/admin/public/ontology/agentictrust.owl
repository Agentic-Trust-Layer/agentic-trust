agentictrust:Tag a owl:Class ;
  rdfs:label "Tag" ;
  rdfs:comment "A tag value used for discovery and matching (e.g., trust.validation, ens.name.valid)." ;
  rdfs:subClassOf prov:Entity .

########
# Intent-driven message model
########

agentictrust:Message a owl:Class ;
  rdfs:label "A2AMessage" ;
  rdfs:comment "A message exchanged between agents; carries content parts." ;
  rdfs:subClassOf prov:Entity .

agentictrust:MessagePart a owl:Class ;
  rdfs:label "MessagePart" ;
  rdfs:comment "Part of a message (text or structured data)." ;
  rdfs:subClassOf prov:Entity .

agentictrust:TextPart a owl:Class ;
  rdfs:label "TextPart" ;
  rdfs:subClassOf agentictrust:MessagePart .

agentictrust:DataPart a owl:Class ;
  rdfs:label "DataPart" ;
  rdfs:subClassOf agentictrust:MessagePart .

agentictrust:Intent a owl:Class ;
  rdfs:label "Intent" ;
  rdfs:comment "Structured intent payload carried in a message data-part. Also treated as a plan (P-PLAN) to support workflow reasoning." ;
  rdfs:subClassOf prov:Entity ;
  rdfs:subClassOf p-plan:Plan .

agentictrust:IntentType a owl:Class ;
  rdfs:label "IntentType" ;
  rdfs:comment "Taxonomy value used to scope discovery and select compatible skills." ;
  rdfs:subClassOf prov:Entity .

agentictrust:IntentAction a owl:Class ;
  rdfs:label "IntentAction" ;
  rdfs:comment "Action/verb used within an intent." ;
  rdfs:subClassOf prov:Entity .

agentictrust:IntentSubject a owl:Class ;
  rdfs:label "IntentSubject" ;
  rdfs:comment "The subject of an intent (e.g., an agent ENS name)." ;
  rdfs:subClassOf prov:Entity .

agentictrust:IntentCheck a owl:Class ;
  rdfs:label "IntentCheck" ;
  rdfs:comment "A requested check within an intent." ;
  rdfs:subClassOf prov:Entity .

agentictrust:ExpectedResult a owl:Class ;
  rdfs:label "ExpectedResult" ;
  rdfs:subClassOf prov:Entity .

agentictrust:Evidence a owl:Class ;
  rdfs:label "Evidence" ;
  rdfs:subClassOf prov:Entity .

agentictrust:Artifact a owl:Class ;
  rdfs:label "ResultArtifact" ;
  rdfs:comment "Artifact produced as a result of executing an intent (e.g., attestation, validation-result)." ;
  rdfs:subClassOf prov:Entity .

agentictrust:Attestation a owl:Class ;
  rdfs:label "Attestation" ;
  rdfs:subClassOf agentictrust:Artifact .

########
# Task / workflow model (P-PLAN + PROV)
########

agentictrust:TaskType a owl:Class ;
  rdfs:label "TaskType" ;
  rdfs:comment "Workflow/task type derived from skills/intent taxonomy." ;
  rdfs:subClassOf prov:Entity .

agentictrust:Task a owl:Class ;
  rdfs:label "Task" ;
  rdfs:comment "A task thread or work item created by intent messaging." ;
  rdfs:subClassOf prov:Entity ;
  rdfs:subClassOf p-plan:Plan .

agentictrust:TaskExecution a owl:Class ;
  rdfs:label "TaskExecution" ;
  rdfs:comment "Execution activity that realizes a task plan." ;
  rdfs:subClassOf prov:Activity .

agentictrust:MessageSend a owl:Class ;
  rdfs:label "MessageSend" ;
  rdfs:subClassOf prov:Activity .

agentictrust:MessageReceive a owl:Class ;
  rdfs:label "MessageReceive" ;
  rdfs:subClassOf prov:Activity .

agentictrust:SkillInvocation a owl:Class ;
  rdfs:label "SkillInvocation" ;
  rdfs:comment "JSON-RPC based invocation of a skill at an A2A service endpoint." ;
  rdfs:subClassOf prov:Activity .

agentictrust:AgentDescriptorFetch a owl:Class ;
  rdfs:label "AgentDescriptorFetch" ;
  rdfs:comment "Indexing activity that retrieves an AgentDescriptor (e.g., A2AAgentCard or MCPManifest) from a registration endpoint." ;
  rdfs:subClassOf prov:Activity .

########
# TrustRegistryModel
########

agentictrust:TrustEvidence a owl:Class ;
  rdfs:label "TrustEvidence" ;
  rdfs:comment "Evidence used to compute or justify trust." ;
