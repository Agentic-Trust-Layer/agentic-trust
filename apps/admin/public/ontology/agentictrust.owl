  rdfs:domain prov:Activity ;
  rdfs:range prov:Plan .

agentictrust:hasFeedback a owl:ObjectProperty ;
  rdfs:label "hasFeedback" ;
  rdfs:domain agentictrust:AIAgent ;
  rdfs:range agentictrust:Feedback .

agentictrust:hasValidation a owl:ObjectProperty ;
  rdfs:label "hasValidation" ;
  rdfs:domain agentictrust:AIAgent ;
  rdfs:range agentictrust:ValidationResponse .

agentictrust:hasAssociation a owl:ObjectProperty ;
