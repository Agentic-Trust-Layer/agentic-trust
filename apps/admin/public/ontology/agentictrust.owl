agentictrust:EndpointType a owl:Class ;
  rdfs:label "EndpointType" ;
  rdfs:comment "Recommended endpoint type taxonomy for agent metadata endpoints." ;
  rdfs:subClassOf prov:Entity .

agentictrust:OperatorIdentifier a owl:Class ;
  rdfs:label "OperatorIdentifier" ;
  rdfs:comment "An identifier for an operator (e.g., address, DID, CAIP-10). Modeled as an Entity (identifier artifact), not a prov:Agent." ;
  rdfs:subClassOf prov:Entity .

agentictrust:AgentDescriptor a owl:Class ;
  rdfs:label "AgentDescriptor" ;
  rdfs:comment "Protocol-agnostic, machine-readable declaration of an agentâ€™s identity, interaction surfaces, protocol bindings, capabilities, and constraints; sufficient for discovery, trust evaluation, and runtime interaction." ;
  rdfs:subClassOf prov:Entity .

agentictrust:A2AAgentCard a owl:Class ;
  rdfs:label "A2AAgentCard" ;
  rdfs:comment "Concrete AgentDescriptor serialization for the A2A protocol (agent.json / agent-card.json)." ;
  rdfs:subClassOf agentictrust:AgentDescriptor .

agentictrust:MCPManifest a owl:Class ;
  rdfs:label "MCPManifest" ;
  rdfs:comment "Concrete AgentDescriptor serialization for the Model Context Protocol (MCP) tool/manifest format." ;
  rdfs:subClassOf agentictrust:AgentDescriptor .

agentictrust:Endpoint a owl:Class ;
  rdfs:label "Endpoint" ;
  rdfs:comment "Network endpoint used by A2A/MCP." ;
  rdfs:subClassOf prov:Entity .

agentictrust:TransportBinding a owl:Class ;
  rdfs:label "TransportBinding" ;
  rdfs:comment "Transport/protocol binding (e.g., HTTP+JSON, JSON-RPC)." ;
  rdfs:subClassOf prov:Entity .

agentictrust:Skill a owl:Class ;
  rdfs:label "Skill" ;
  rdfs:comment "A callable capability (tool/function) exposed by an agent. Used across protocols (e.g., A2A skills, MCP tools)." ;
