  rdfs:range xsd:string .

########
# Core Agent / Agent Card concepts
########

########
# ERC8004VocabularyLayer
########

agentictrust:AIAgent a owl:Class ;
  rdfs:label "AIAgent" ;
  rdfs:comment "AI agent identity." ;
  rdfs:subClassOf prov:SoftwareAgent ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty agentictrust:hasDescriptor ;
    owl:allValuesFrom agentictrust:AgentDescriptor
  ] .

agentictrust:EndpointType a owl:Class ;
  rdfs:label "EndpointType" ;
  rdfs:comment "Recommended endpoint type taxonomy for agent metadata endpoints (e.g., a2a, mcp, ens, agentAccount)." ;
  rdfs:subClassOf prov:Entity .

agentictrust:OperatorIdentifier a owl:Class ;
  rdfs:label "OperatorIdentifier" ;
  rdfs:comment "An identifier for an operator (e.g., address, DID, CAIP-10). Subclass of DecentralizedIdentifier (identifier artifact)." ;
  rdfs:subClassOf agentictrust:DecentralizedIdentifier .

agentictrust:Descriptor a owl:Class ;
  rdfs:label "Descriptor" ;
  rdfs:comment "A resolver-produced description that aggregates and normalizes all metadata associated with a particular thing (AIAgent, Identifier, Protocol). Represents the resolved/operational view used for discovery, validation, and interaction." ;
  rdfs:subClassOf prov:Entity .

agentictrust:IdentifierDescriptor a owl:Class ;
  rdfs:label "IdentifierDescriptor" ;
  rdfs:comment "Descriptor for an Identifier (resolved metadata about an identifier and its bindings, subject/controller links, and verification methods)." ;
  rdfs:subClassOf agentictrust:Descriptor .

agentictrust:AgentIdentityDescriptor a owl:Class ;
  rdfs:label "AgentIdentityDescriptor" ;
  rdfs:comment "Descriptor for an AgentIdentity (resolved metadata about an identity, its identifiers, and associated metadata). Protocol-specific realizations include erc8004:IdentityDescriptor8004, NANDAIdentityDescriptor, etc." ;
  rdfs:subClassOf agentictrust:Descriptor .

agentictrust:AgentNameDescriptor a owl:Class ;
  rdfs:label "AgentNameDescriptor" ;
  rdfs:comment "Descriptor for an AgentName (resolved metadata about a name, its identifiers, and associated metadata). Protocol-specific realizations include agentictrustEth:AgentNameENSDescriptor, DNSNameDescriptor, etc." ;
  rdfs:subClassOf agentictrust:Descriptor .


agentictrust:AgentDescriptor a owl:Class ;
  rdfs:label "AgentDescriptor" ;
  rdfs:comment "Agent-level descriptor: resolver-produced, protocol-agnostic declaration of an agent's identity, capabilities, and constraints. Belongs to an Agent, not a Protocol. Distinct from ProtocolDescriptor." ;
  rdfs:subClassOf agentictrust:Descriptor .

agentictrust:Protocol a owl:Class ;
  rdfs:label "Protocol" ;
  rdfs:comment "A communication protocol (e.g., A2A, MCP). Has a ProtocolDescriptor that describes the protocol-specific configuration and bindings." ;
  rdfs:subClassOf prov:Entity ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty agentictrust:hasDescriptor ;
    owl:allValuesFrom agentictrust:ProtocolDescriptor
  ] .

agentictrust:ProtocolDescriptor a owl:Class ;
  rdfs:label "ProtocolDescriptor" ;
  rdfs:comment "Protocol-level descriptor: resolver-produced protocol-specific configuration, bindings, and metadata. Belongs to a Protocol, not an Agent. Distinct from AgentDescriptor. Can be A2AProtocolDescriptor or MCPProtocolDescriptor." ;
  rdfs:subClassOf agentictrust:Descriptor .

agentictrust:A2AProtocolDescriptor a owl:Class ;
  rdfs:label "A2AProtocolDescriptor" ;
  rdfs:comment "Protocol descriptor for the A2A protocol (agent.json / agent-card.json format). Describes A2A-specific protocol configuration and bindings." ;
  rdfs:subClassOf agentictrust:ProtocolDescriptor .

agentictrust:MCPProtocolDescriptor a owl:Class ;
  rdfs:label "MCPProtocolDescriptor" ;
  rdfs:comment "Protocol descriptor for the Model Context Protocol (MCP) tool/manifest format. Describes MCP-specific protocol configuration and bindings." ;
  rdfs:subClassOf agentictrust:ProtocolDescriptor .

agentictrust:Endpoint a owl:Class ;
  rdfs:label "Endpoint" ;
  rdfs:comment "Network endpoint used by A2A/MCP." ;
  rdfs:subClassOf prov:Entity .

agentictrust:AgentSkill a owl:Class ;
  rdfs:label "AgentSkill" ;
  rdfs:comment "An agent-declared skill instance (part of a Descriptor). This is the concrete skill node used in RDF exports, which can be classified using AgentSkillClassification (e.g., OASFSkill)." ;
  rdfs:subClassOf prov:Entity .

agentictrust:TransportBinding a owl:Class ;
  rdfs:label "TransportBinding" ;
  rdfs:comment "Transport/protocol binding (e.g., HTTP+JSON, JSON-RPC)." ;
  rdfs:subClassOf prov:Entity .

agentictrust:AgentSkillClassification a owl:Class ;
  rdfs:label "AgentSkillClassification" ;
  rdfs:comment "A callable capability (tool/function) classification exposed by an agent. Used across protocols (e.g., A2A skills, MCP tools). Follows OASF (Open Agent Skill Format) standards for skill classification and discovery." ;
  rdfs:subClassOf prov:Entity .

agentictrust:JsonSchema a owl:Class ;
  rdfs:label "JsonSchema" ;
  rdfs:comment "A JSON Schema artifact describing inputs/outputs for a callable capability." ;
  rdfs:subClassOf prov:Entity .

agentictrust:SkillExample a owl:Class ;
  rdfs:label "SkillExample" ;
  rdfs:comment "Example attached to an agent skill classification; may contain intent-driven requests." ;
  rdfs:subClassOf prov:Entity .

agentictrust:Tag a owl:Class ;
  rdfs:label "Tag" ;
  rdfs:comment "A tag value used for discovery and matching (e.g., trust.validation, ens.name.valid)." ;
  rdfs:subClassOf prov:Entity .

agentictrust:AgentDomain a owl:Class ;
  rdfs:label "AgentDomain" ;
  rdfs:comment "An agent-declared domain instance (part of a Descriptor). This is the concrete domain node used in RDF exports, which can be classified using AgentDomainClassification (e.g., OASFDomain)." ;
  rdfs:subClassOf prov:Entity .

agentictrust:AgentDomainClassification a owl:Class ;
  rdfs:label "AgentDomainClassification" ;
  rdfs:comment "OASF Domain classification: A categorization/grouping mechanism for discovery and filtering. Examples: 'finance', 'healthcare', 'education', 'e-commerce'. Distinct from integration capabilities (which describe how agents integrate with external systems). Follows OASF (Open Agent Skill Format) standards. Synced from https://github.com/agntcy/oasf/tree/main/schema/domains" ;
  rdfs:subClassOf prov:Entity .

agentictrust:OASFDomain a owl:Class ;
  rdfs:label "OASFDomain" ;
  rdfs:comment "OASF Domain instance synced from GitHub. Contains OASF schema metadata including name, description, category, and full schema JSON." ;
  rdfs:subClassOf agentictrust:AgentDomainClassification ;
  rdfs:subClassOf prov:Entity .

agentictrust:OASFSkill a owl:Class ;
  rdfs:label "OASFSkill" ;
  rdfs:comment "OASF Skill instance synced from GitHub. Contains OASF schema metadata including name, description, domain, category, and full schema JSON. Synced from https://github.com/agntcy/oasf/tree/main/schema/skills" ;
  rdfs:subClassOf agentictrust:AgentSkillClassification ;
  rdfs:subClassOf prov:Entity .

########
# Intent-driven message model
########

agentictrust:Message a owl:Class ;
  rdfs:label "A2AMessage" ;
  rdfs:comment "A message exchanged between agents; carries content parts." ;
  rdfs:subClassOf prov:Entity .

agentictrust:MessagePart a owl:Class ;
  rdfs:label "MessagePart" ;
  rdfs:comment "Part of a message (text or structured data)." ;
  rdfs:subClassOf prov:Entity .

agentictrust:TextPart a owl:Class ;
  rdfs:label "TextPart" ;
  rdfs:subClassOf agentictrust:MessagePart .

agentictrust:DataPart a owl:Class ;
  rdfs:label "DataPart" ;
  rdfs:subClassOf agentictrust:MessagePart .

agentictrust:Intent a owl:Class ;
  rdfs:label "Intent" ;
  rdfs:comment "Structured intent payload carried in a message data-part. Also treated as a plan (P-PLAN) to support workflow reasoning." ;
  rdfs:subClassOf prov:Entity ;
  rdfs:subClassOf p-plan:Plan .

agentictrust:IntentType a owl:Class ;
  rdfs:label "IntentType" ;
  rdfs:comment "A description of why an agent capability is being invoked. Intent explains why a skill is invoked, not how it is executed. Modeled as a SituationDescription (PROV Entity / Plan-like schema)." ;
  rdfs:subClassOf agentictrust:SituationDescription .

agentictrust:IntentSituation a owl:Class ;
  rdfs:label "IntentSituation" ;
  rdfs:comment "A concrete epistemic situation in which an agent expresses or acts under a given intent. Modeled as a Situation (prov:Entity) that can be realized by Activities (see agentictrust:isRealizedBy)." ;
  rdfs:subClassOf agentictrust:Situation .

agentictrust:IntentAction a owl:Class ;
  rdfs:label "IntentAction" ;
  rdfs:comment "Action/verb used within an intent." ;
  rdfs:subClassOf prov:Entity .

agentictrust:IntentSubject a owl:Class ;
  rdfs:label "IntentSubject" ;
  rdfs:comment "The subject of an intent (e.g., an agent ENS name)." ;
  rdfs:subClassOf prov:Entity .

agentictrust:IntentCheck a owl:Class ;
  rdfs:label "IntentCheck" ;
  rdfs:comment "A requested check within an intent." ;
  rdfs:subClassOf prov:Entity .

agentictrust:ExpectedResult a owl:Class ;
  rdfs:label "ExpectedResult" ;
  rdfs:subClassOf prov:Entity .

agentictrust:Evidence a owl:Class ;
  rdfs:label "Evidence" ;
  rdfs:subClassOf prov:Entity .

agentictrust:Artifact a owl:Class ;
  rdfs:label "ResultArtifact" ;
  rdfs:comment "Artifact produced as a result of executing an intent (e.g., attestation, validation-result)." ;
  rdfs:subClassOf prov:Entity .

agentictrust:Attestation a owl:Class ;
  rdfs:label "Attestation" ;
  rdfs:subClassOf agentictrust:Artifact .

########
# Task / workflow model (P-PLAN + PROV)
########

agentictrust:TaskType a owl:Class ;
  rdfs:label "TaskType" ;
  rdfs:comment "Workflow/task type derived from skills/intent taxonomy." ;
  rdfs:subClassOf prov:Entity .

agentictrust:Task a owl:Class ;
  rdfs:label "Task" ;
  rdfs:comment "A task thread or work item created by intent messaging." ;
  rdfs:subClassOf prov:Entity ;
  rdfs:subClassOf p-plan:Plan .

agentictrust:TaskExecution a owl:Class ;
  rdfs:label "TaskExecution" ;
  rdfs:comment "Execution activity that realizes a task plan." ;
  rdfs:subClassOf prov:Activity .

agentictrust:MessageSend a owl:Class ;
  rdfs:label "MessageSend" ;
  rdfs:subClassOf prov:Activity .

agentictrust:MessageReceive a owl:Class ;
  rdfs:label "MessageReceive" ;
  rdfs:subClassOf prov:Activity .

agentictrust:SkillInvocation a owl:Class ;
  rdfs:label "SkillInvocation" ;
  rdfs:comment "JSON-RPC based invocation of a skill at an A2A service endpoint." ;
  rdfs:subClassOf prov:Activity .

agentictrust:AgentDescriptorFetch a owl:Class ;
  rdfs:label "AgentDescriptorFetch" ;
  rdfs:comment "Indexing activity that retrieves an AgentDescriptor from a registration endpoint." ;
  rdfs:subClassOf prov:Activity .

########
# TrustRegistryModel
########

agentictrust:TrustEvidence a owl:Class ;
  rdfs:label "TrustEvidence" ;
  rdfs:comment "Evidence used to compute or justify trust." ;
  rdfs:subClassOf agentictrust:Evidence .

agentictrust:TrustArtifact a owl:Class ;
  rdfs:label "TrustArtifact" ;
  rdfs:comment "Artifact produced by trust workflows (e.g., validation response)." ;
  rdfs:subClassOf agentictrust:Artifact .

