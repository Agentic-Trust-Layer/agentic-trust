  rdfs:comment "Authority/institution model: confidence comes from a recognized authority vouching (government registries, corporate PKI, auditors/certifiers, regulated entities)." .

agentictrust:TrustModel_identity_assurance a agentictrust:TrustModel ;
  rdfs:label "identity-assurance" ;
  agentictrust:trustModelValue "identity-assurance" ;
  rdfs:comment "Identity proofing model: confidence comes from strength of binding between the agent and a real entity (DID method, KYC/KYB attestations, hardware-bound keys, multisig control)." .

agentictrust:TrustModel_process_conformance a agentictrust:TrustModel ;
  rdfs:label "process-conformance" ;
  agentictrust:trustModelValue "process-conformance" ;
  rdfs:comment "Compliance model: confidence comes from adherence to predefined workflows/plans (PROV-O traces, P-Plan alignment, audit trails)." .

agentictrust:TrustModel_data_provenance a agentictrust:TrustModel ;
