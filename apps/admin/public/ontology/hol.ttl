@base <https://agentictrust.io/ontology/hol#> .
@prefix : <https://agentictrust.io/ontology/hol#> .
@prefix core: <https://agentictrust.io/ontology/core#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://agentictrust.io/ontology/hol> a owl:Ontology ;
  rdfs:label "Agentic Trust HOL Ontology" ;
  owl:versionInfo "0.1.0" ;
  owl:imports <https://agentictrust.io/ontology/core> ;
  owl:imports <https://agentictrust.io/ontology/core/identity> ;
  owl:imports <https://agentictrust.io/ontology/core/descriptors> ;
  owl:imports <http://www.w3.org/ns/prov-o#> .

########
# HOL agent class
########

:AIAgentHOL a owl:Class ;
  rdfs:label "AIAgentHOL" ;
  rdfs:comment "AI agent discovered/represented via HOL (Hashgraph Online)." ;
  rdfs:subClassOf core:AIAgent ;
  rdfs:subClassOf prov:SoftwareAgent .

########
# HOL identity layer
########

:AgentIdentityRegistryHOL a owl:Class ;
  rdfs:label "AgentIdentityRegistryHOL" ;
  rdfs:subClassOf core:AgentRegistry ;
  rdfs:subClassOf prov:Entity .

:AgentIdentityRegistrationHOL a owl:Class ;
  rdfs:label "AgentIdentityRegistrationHOL" ;
  rdfs:comment "Registration record anchoring an HOL identity within an HOL registry." ;
  rdfs:subClassOf core:AgentIdentityRegistration ;
  rdfs:subClassOf prov:Entity .

:AgentIdentityHOL a owl:Class ;
  rdfs:label "AgentIdentityHOL" ;
  rdfs:subClassOf core:AgentIdentity ;
  rdfs:subClassOf prov:Entity .

:IdentityIdentifierHOL a owl:Class ;
  rdfs:label "IdentityIdentifierHOL" ;
  rdfs:subClassOf core:UniversalIdentifier ;
  rdfs:subClassOf prov:Entity .

:IdentityDescriptorHOL a owl:Class ;
  rdfs:label "IdentityDescriptorHOL" ;
  rdfs:subClassOf core:AgentIdentityDescriptor .

:AgentProfileHOL a owl:Class ;
  rdfs:label "AgentProfileHOL" ;
  rdfs:subClassOf prov:Entity .

########
# HOL search-hit descriptor (registry-produced view)
########

:HOLAgentDescriptor a owl:Class ;
  rdfs:label "HOLAgentDescriptor" ;
  rdfs:comment "Descriptor produced from HOL registry search results (e.g., hol.org/api/v1/search)." ;
  rdfs:subClassOf core:AgentDescriptor ;
  rdfs:subClassOf prov:Entity .

:hasHOLAgentDescriptor a owl:ObjectProperty ;
  rdfs:label "hasHOLAgentDescriptor" ;
  rdfs:comment "Convenience property linking an agent to a HOLAgentDescriptor (subPropertyOf core:hasDescriptor)." ;
  rdfs:subPropertyOf core:hasDescriptor ;
  rdfs:domain core:AIAgent ;
  rdfs:range :HOLAgentDescriptor .

:hasAgentProfileHOL a owl:ObjectProperty ;
  rdfs:label "hasAgentProfileHOL" ;
  rdfs:domain :AgentIdentityHOL ;
  rdfs:range :AgentProfileHOL .

:uaid a owl:DatatypeProperty ;
  rdfs:label "uaid" ;
  rdfs:domain :AgentProfileHOL ;
  rdfs:range xsd:string .

:uaidHOL a owl:DatatypeProperty ;
  rdfs:label "uaidHOL" ;
  rdfs:comment "Original UAID string ingested from HOL sources (including any routing params after ';')." ;
  rdfs:domain :AgentIdentityHOL ;
  rdfs:range xsd:string .

:originalId a owl:DatatypeProperty ;
  rdfs:label "originalId" ;
  rdfs:domain :AgentProfileHOL ;
  rdfs:range xsd:string .

:oasfSkillUid a owl:DatatypeProperty ;
  rdfs:label "oasfSkillUid" ;
  rdfs:comment "OASF skill UID (integer) extracted from HOL payloads (e.g. metadata.oasfSkills, profile.properties.oasfSkills)." ;
  rdfs:domain :AgentProfileHOL ;
  rdfs:range xsd:integer .

:capabilityLabel a owl:DatatypeProperty ;
  rdfs:label "capabilityLabel" ;
  rdfs:comment "Capability label tokens extracted from HOL payloads (e.g. metadata.capabilityLabels)." ;
  rdfs:domain :AgentProfileHOL ;
  rdfs:range xsd:string .

:capabilityToken a owl:DatatypeProperty ;
  rdfs:label "capabilityToken" ;
  rdfs:comment "Capability tokens extracted from HOL payloads (e.g. capabilityTokens array)." ;
  rdfs:domain :AgentProfileHOL ;
  rdfs:range xsd:string .

:protocol a owl:DatatypeProperty ;
  rdfs:label "protocol" ;
  rdfs:comment "Protocol identifiers extracted from HOL payloads (e.g. protocols array)." ;
  rdfs:domain :AgentProfileHOL ;
  rdfs:range xsd:string .

:trustScoresJson a owl:DatatypeProperty ;
  rdfs:label "trustScoresJson" ;
  rdfs:comment "Raw trustScores object JSON extracted from HOL payloads." ;
  rdfs:domain :AgentProfileHOL ;
  rdfs:range xsd:string .

:registry a owl:DatatypeProperty ;
  rdfs:label "registry" ;
  rdfs:domain :AgentProfileHOL ;
  rdfs:range xsd:string .

:displayName a owl:DatatypeProperty ;
  rdfs:label "displayName" ;
  rdfs:domain :AgentProfileHOL ;
  rdfs:range xsd:string .

:bio a owl:DatatypeProperty ;
  rdfs:label "bio" ;
  rdfs:domain :AgentProfileHOL ;
  rdfs:range xsd:string .

:rating a owl:DatatypeProperty ;
  rdfs:label "rating" ;
  rdfs:domain :AgentProfileHOL ;
  rdfs:range xsd:decimal .

:trustScore a owl:DatatypeProperty ;
  rdfs:label "trustScore" ;
  rdfs:domain :AgentProfileHOL ;
  rdfs:range xsd:decimal .

:totalInteractions a owl:DatatypeProperty ;
  rdfs:label "totalInteractions" ;
  rdfs:domain :AgentProfileHOL ;
  rdfs:range xsd:integer .

:availabilityScore a owl:DatatypeProperty ;
  rdfs:label "availabilityScore" ;
  rdfs:domain :AgentProfileHOL ;
  rdfs:range xsd:decimal .

:availabilityLatencyMs a owl:DatatypeProperty ;
  rdfs:label "availabilityLatencyMs" ;
  rdfs:domain :AgentProfileHOL ;
  rdfs:range xsd:integer .

:available a owl:DatatypeProperty ;
  rdfs:label "available" ;
  rdfs:domain :AgentProfileHOL ;
  rdfs:range xsd:boolean .

:lastIndexed a owl:DatatypeProperty ;
  rdfs:label "lastIndexed" ;
  rdfs:domain :AgentProfileHOL ;
  rdfs:range xsd:dateTime .

:lastSeen a owl:DatatypeProperty ;
  rdfs:label "lastSeen" ;
  rdfs:domain :AgentProfileHOL ;
  rdfs:range xsd:dateTime .

########
# HOL capability / endpoint / relationship / validation (resolved payload)
########

:CapabilityHOL a owl:Class ;
  rdfs:label "CapabilityHOL" ;
  rdfs:comment "Capability entry as returned by HOL registry resolve/search payloads. Stored as a first-class node so we can normalize and query." ;
  rdfs:subClassOf prov:Entity .

:hasCapabilityHOL a owl:ObjectProperty ;
  rdfs:label "hasCapabilityHOL" ;
  rdfs:comment "Links an HOL agent profile to capability nodes retrieved from HOL registry resolve/search payloads." ;
  rdfs:domain :AgentProfileHOL ;
  rdfs:range :CapabilityHOL .

:RelationshipHOL a owl:Class ;
  rdfs:label "RelationshipHOL" ;
  rdfs:comment "Relationship entry as returned by HOL registry resolve payloads. Stored as an opaque node (core:json) until normalized with core relationship models." ;
  rdfs:subClassOf prov:Entity .

:hasRelationshipHOL a owl:ObjectProperty ;
  rdfs:label "hasRelationshipHOL" ;
  rdfs:comment "Links an HOL agent profile to relationship nodes retrieved from HOL registry resolve payloads." ;
  rdfs:domain :AgentProfileHOL ;
  rdfs:range :RelationshipHOL .

:ValidationHOL a owl:Class ;
  rdfs:label "ValidationHOL" ;
  rdfs:comment "Validation entry as returned by HOL registry resolve payloads. Stored as an opaque node (core:json) until normalized with core verification/validation models." ;
  rdfs:subClassOf prov:Entity .

:hasValidationHOL a owl:ObjectProperty ;
  rdfs:label "hasValidationHOL" ;
  rdfs:comment "Links an HOL agent profile to validation nodes retrieved from HOL registry resolve payloads." ;
  rdfs:domain :AgentProfileHOL ;
  rdfs:range :ValidationHOL .
