@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix p-plan: <http://purl.org/net/p-plan#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix agentictrust: <https://www.agentictrust.io/ontology/agentictrust-core#> .
@prefix agentictrustEth: <https://www.agentictrust.io/ontology/agentictrust-eth#> .
@prefix erc8092: <https://www.agentictrust.io/ontology/ERC8092#> .

erc8092: a owl:Ontology ;
  dcterms:title "ERC8092Ontology" ;
  dcterms:description "ERC-8092 aligned terms for relationship assertions and relationship accounts, layered on the AgenticTrust core trust model." ;
  owl:imports <https://www.agentictrust.io/ontology/agentictrust-core> ;
  owl:imports <https://www.agentictrust.io/ontology/agentictrust-eth> ;
  owl:imports <http://www.w3.org/ns/prov-o#> ;
  owl:imports <http://purl.org/net/p-plan#> ;
  owl:versionInfo "0.1.0" ;
  dcterms:license <https://opensource.org/license/mit/> .

########
# ERC-8092 Relationship model
########

erc8092:ERC8092Relationship a owl:Class ;
  rdfs:label "ERC8092Relationship" ;
  rdfs:comment "An ERC-8092 relationship instance (identified by associationId). Specializes AccountRelationship for ERC-8092 account-to-account relationships." ;
  rdfs:subClassOf agentictrustEth:AccountRelationship .

erc8092:ERC8092AccountRelationship a owl:Class ;
  rdfs:label "ERC8092AccountRelationship" ;
  rdfs:comment "An ERC-8092 account relationship instance (identified by associationId). Alias/synonym for ERC8092Relationship, emphasizing the account-to-account nature. Instances are typed with both classes in RDF data." ;
  rdfs:subClassOf agentictrustEth:AccountRelationship .

erc8092:ERC8092RelationshipAssertion a owl:Class ;
  rdfs:label "ERC8092RelationshipAssertion" ;
  rdfs:comment "An ERC-8092 association record interpreted as an account relationship assertion that establishes or modifies an underlying AccountRelationship." ;
  rdfs:subClassOf agentictrustEth:AccountRelationshipAssertion .

erc8092:ERC8092AccountRelationshipAssertion a owl:Class ;
  rdfs:label "ERC8092AccountRelationshipAssertion" ;
  rdfs:comment "An ERC-8092 account relationship assertion. Alias/synonym for ERC8092RelationshipAssertion, emphasizing the account-to-account nature. Instances are typed with both classes in RDF data." ;
  rdfs:subClassOf agentictrustEth:AccountRelationshipAssertion .

erc8092:RelationshipAccount a owl:Class ;
  rdfs:label "RelationshipAccount" ;
  rdfs:comment "An ERC-8092 account identifier (e.g., key-id / account-id such as 0x000100...). This is not necessarily an EVM address. Specializes AccountRelationshipAccount." ;
  rdfs:subClassOf agentictrustEth:AccountRelationshipAccount .

erc8092:RelationshipRevocationAssertion a owl:Class ;
  rdfs:label "RelationshipRevocationAssertion" ;
  rdfs:comment "A revocation assertion that modifies an AccountRelationship by revoking a prior AccountRelationshipAssertion." ;
  rdfs:subClassOf agentictrustEth:AccountRelationshipAssertion .

########
# ERC-8092 properties
########

erc8092:relationshipId a owl:DatatypeProperty ;
  rdfs:label "relationshipId" ;
  rdfs:domain agentictrustEth:AccountRelationship ;
  rdfs:range xsd:string .

erc8092:relationshipAssertionId a owl:DatatypeProperty ;
  rdfs:label "relationshipAssertionId" ;
  rdfs:domain agentictrustEth:AccountRelationshipAssertion ;
  rdfs:range xsd:string .

erc8092:initiator a owl:ObjectProperty ;
  rdfs:label "initiator" ;
  rdfs:comment "The initiator Account (may be a key-id/account-id represented as an Account IRI in data)." ;
  rdfs:domain erc8092:ERC8092RelationshipAssertion ;
  rdfs:range agentictrustEth:Account .

erc8092:approver a owl:ObjectProperty ;
  rdfs:label "approver" ;
  rdfs:comment "The approver Account (may be a key-id/account-id represented as an Account IRI in data)." ;
  rdfs:domain erc8092:ERC8092RelationshipAssertion ;
  rdfs:range agentictrustEth:Account .

erc8092:initiatorAccount a owl:ObjectProperty ;
  rdfs:label "initiatorAccount" ;
  rdfs:domain erc8092:ERC8092RelationshipAssertion ;
  rdfs:range agentictrustEth:AccountRelationshipAccount .

erc8092:approverAccount a owl:ObjectProperty ;
  rdfs:label "approverAccount" ;
  rdfs:domain erc8092:ERC8092RelationshipAssertion ;
  rdfs:range agentictrustEth:AccountRelationshipAccount .

erc8092:ownsRelationshipAccount a owl:ObjectProperty ;
  rdfs:label "ownsRelationshipAccount" ;
  rdfs:comment "Links an Account to relationship account ids it controls/uses in ERC-8092 relationships." ;
  rdfs:subPropertyOf agentictrustEth:ownsAccountRelationshipAccount ;
  rdfs:domain agentictrustEth:Account ;
  rdfs:range agentictrustEth:AccountRelationshipAccount .

erc8092:hasRelationshipAssertion a owl:ObjectProperty ;
  rdfs:label "hasRelationshipAssertion" ;
  rdfs:comment "ERC-8092 specific property linking Accounts to account relationship assertions (associations) they participate in. Subproperty of agentictrustEth:hasAccountRelationshipAssertion." ;
  rdfs:subPropertyOf agentictrustEth:hasAccountRelationshipAssertion ;
  rdfs:domain agentictrustEth:Account ;
  rdfs:range erc8092:ERC8092RelationshipAssertion .

erc8092:relationshipAccountId a owl:DatatypeProperty ;
  rdfs:label "relationshipAccountId" ;
  rdfs:comment "Raw relationship account identifier string (DB id column)." ;
  rdfs:domain erc8092:RelationshipAccount ;
  rdfs:range xsd:string .

erc8092:accountAddress a owl:DatatypeProperty ;
  rdfs:label "accountAddress" ;
  rdfs:comment "Alias of relationshipAccountId for query convenience (maps to the same underlying value)." ;
  rdfs:domain erc8092:RelationshipAccount ;
  rdfs:range xsd:string .

erc8092:interfaceId a owl:DatatypeProperty ;
  rdfs:label "interfaceId" ;
  rdfs:domain erc8092:ERC8092RelationshipAssertion ;
  rdfs:range xsd:string .

erc8092:validAt a owl:DatatypeProperty ;
  rdfs:label "validAt" ;
  rdfs:domain erc8092:ERC8092RelationshipAssertion ;
  rdfs:range xsd:integer .

erc8092:validUntil a owl:DatatypeProperty ;
  rdfs:label "validUntil" ;
  rdfs:domain erc8092:ERC8092RelationshipAssertion ;
  rdfs:range xsd:integer .

erc8092:revokedAt a owl:DatatypeProperty ;
  rdfs:label "revokedAt" ;
  rdfs:domain erc8092:ERC8092RelationshipAssertion ;
  rdfs:range xsd:integer .

erc8092:dataHex a owl:DatatypeProperty ;
  rdfs:label "dataHex" ;
  rdfs:domain erc8092:ERC8092RelationshipAssertion ;
  rdfs:range xsd:string .

erc8092:initiatorKeyType a owl:DatatypeProperty ;
  rdfs:label "initiatorKeyType" ;
  rdfs:domain erc8092:ERC8092RelationshipAssertion ;
  rdfs:range xsd:string .

erc8092:approverKeyType a owl:DatatypeProperty ;
  rdfs:label "approverKeyType" ;
  rdfs:domain erc8092:ERC8092RelationshipAssertion ;
  rdfs:range xsd:string .

erc8092:initiatorSignature a owl:DatatypeProperty ;
  rdfs:label "initiatorSignature" ;
  rdfs:domain erc8092:ERC8092RelationshipAssertion ;
  rdfs:range xsd:string .

erc8092:approverSignature a owl:DatatypeProperty ;
  rdfs:label "approverSignature" ;
  rdfs:domain erc8092:ERC8092RelationshipAssertion ;
  rdfs:range xsd:string .

erc8092:createdTxHash a owl:DatatypeProperty ;
  rdfs:label "createdTxHash" ;
  rdfs:domain erc8092:ERC8092RelationshipAssertion ;
  rdfs:range xsd:string .

erc8092:createdBlockNumber a owl:DatatypeProperty ;
  rdfs:label "createdBlockNumber" ;
  rdfs:domain erc8092:ERC8092RelationshipAssertion ;
  rdfs:range xsd:integer .

erc8092:createdTimestamp a owl:DatatypeProperty ;
  rdfs:label "createdTimestamp" ;
  rdfs:domain erc8092:ERC8092RelationshipAssertion ;
  rdfs:range xsd:integer .

erc8092:lastUpdatedTxHash a owl:DatatypeProperty ;
  rdfs:label "lastUpdatedTxHash" ;
  rdfs:domain erc8092:ERC8092RelationshipAssertion ;
  rdfs:range xsd:string .

erc8092:lastUpdatedBlockNumber a owl:DatatypeProperty ;
  rdfs:label "lastUpdatedBlockNumber" ;
  rdfs:domain erc8092:ERC8092RelationshipAssertion ;
  rdfs:range xsd:integer .

erc8092:lastUpdatedTimestamp a owl:DatatypeProperty ;
  rdfs:label "lastUpdatedTimestamp" ;
  rdfs:domain erc8092:ERC8092RelationshipAssertion ;
  rdfs:range xsd:integer .

erc8092:revocationOfRelationshipAssertion a owl:ObjectProperty ;
  rdfs:label "revocationOfRelationshipAssertion" ;
  rdfs:domain erc8092:RelationshipRevocationAssertion ;
  rdfs:range agentictrustEth:AccountRelationshipAssertion .

erc8092:revocationTxHash a owl:DatatypeProperty ;
  rdfs:label "revocationTxHash" ;
  rdfs:domain erc8092:RelationshipRevocationAssertion ;
  rdfs:range xsd:string .

erc8092:revocationBlockNumber a owl:DatatypeProperty ;
  rdfs:label "revocationBlockNumber" ;
  rdfs:domain erc8092:RelationshipRevocationAssertion ;
  rdfs:range xsd:integer .

erc8092:revocationTimestamp a owl:DatatypeProperty ;
  rdfs:label "revocationTimestamp" ;
  rdfs:domain erc8092:RelationshipRevocationAssertion ;
  rdfs:range xsd:integer .

########
# Relationship Participant and Role properties (from agentictrust-core)
########

erc8092:hasParticipant a owl:ObjectProperty ;
  rdfs:label "hasParticipant" ;
  rdfs:comment "ERC-8092 specific property linking a Relationship to an Agent that participates in it. Subproperty of agentictrust:hasParticipant." ;
  rdfs:subPropertyOf agentictrust:hasParticipant ;
  rdfs:domain erc8092:ERC8092Relationship ;
  rdfs:range agentictrust:AIAgent .

erc8092:qualifiedParticipation a owl:ObjectProperty ;
  rdfs:label "qualifiedParticipation" ;
  rdfs:comment "ERC-8092 specific property linking a Relationship to a QualifiedParticipation instance. Subproperty of agentictrust:qualifiedParticipation." ;
  rdfs:subPropertyOf agentictrust:qualifiedParticipation ;
  rdfs:domain erc8092:ERC8092Relationship ;
  rdfs:range agentictrust:QualifiedParticipation .

erc8092:hasRole a owl:ObjectProperty ;
  rdfs:label "hasRole" ;
  rdfs:comment "ERC-8092 specific property linking a Relationship to a Role. Subproperty of agentictrust:hasRole." ;
  rdfs:subPropertyOf agentictrust:hasRole ;
  rdfs:domain erc8092:ERC8092Relationship ;
  rdfs:range p-plan:Role .


